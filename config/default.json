{
  "server": {
    "port": 3000,
    "host": "localhost"
  },
  "display": {
    "reference": {
      "width": 1280,
      "height": 800
    }
  },
  "logging": {
    "level": "info",
    "filename": "logs/winnieos.log"
  },
  "apps": {
    "enabled": [
      "notepad",
      "letters",
      "colors"
    ]
  },
  "kiosk": {
    "user": "WinnieOSKiosk"
  },
  "startup": {
    "gitPull": true,
    "buildOnChanges": true,
    "buildOnMissing": true
  }
}
