{
  "name": "winnieos",
  "version": "1.0.0",
  "description": "A kid-friendly computing environment optimized for learning basic computing concepts",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "stop:dev": "powershell -ExecutionPolicy Bypass -File scripts/stop-dev.ps1",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "install-service": "node scripts/install-service.js install",
    "uninstall-service": "node scripts/install-service.js uninstall"
  },
  "keywords": [
    "kiosk",
    "education",
    "toddler",
    "computing"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "express": "^5.2.1",
    "matter-js": "^0.20.0",
    "node-windows": "^1.0.0-beta.8",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "jsdom": "^25.0.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
